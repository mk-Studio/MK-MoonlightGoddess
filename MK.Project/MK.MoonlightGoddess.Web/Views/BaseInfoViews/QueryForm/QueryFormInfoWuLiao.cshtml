
@{
    Layout = null;
}
<!--QueryFormInfoWuLiao-->
<!--Row1-->
<div class="layui-form-item">
    <label class="layui-form-label">物料类别</label>
    <div class="layui-input-inline">
        <select id="WuLiaoTypeID" name="WuLiaoTypeID" lay-verify="required" lay-search="" lay-filter="WuLiaoTypeID"></select>
    </div>

    <label class="layui-form-label">商品名称</label>
    <div class="layui-input-inline">
        <input type="text" name="ShangPinName" lay-verify="" autocomplete="off" class="layui-input">
    </div>

</div>

<!--Row2-->
<div class="layui-form-item">
    <label class="layui-form-label">规格</label>
    <div class="layui-input-inline">
        <input type="text" name="GuiGe" lay-verify="" autocomplete="off" class="layui-input">
    </div>

    <label class="layui-form-label">海关编号</label>
    <div class="layui-input-inline">
        <input type="text" name="HaiGuanNo" lay-verify="" autocomplete="off" class="layui-input">
    </div>

    <div class="layui-input-inline">
        <button class="layui-btn" lay-submit lay-filter="comQuery"><i class="layui-icon layui-icon-search" style="font-size: 15px;"></i> 查询</button>
    </div>
</div>

<div class="layui-form-item">
    <label class="layui-form-label">创建时间</label>
    <div class="layui-input-inline">
        <input type="text" name="StartDate" class="layui-input queryDate" placeholder="起始日期">
    </div>
    <div class="layui-form-mid">~</div>
    <div class="layui-input-inline">
        <input type="text" name="EndDate" class="layui-input queryDate2" placeholder="结束日期">
    </div>

    <label class="layui-form-label">显示标识</label>
    <div class="layui-input-inline">
        <select id="ShowMark" name="ShowMark" lay-verify="required" lay-search="" lay-filter="ShowMark">
            <option value="All">全部</option>
            <option value="Y">显示</option>
            <option value="N">不显示</option>
        </select>
    </div>
</div>
